{
  "$schema": "../schema.json",
  "name": "proto",
  "repo": "aquasecurity/trivy",
  "assets": {
    "Linux": {
      "X64": "trivy_{version}_Linux-64bit.tar.gz",
      "ARM64": "trivy_{version}_Linux-ARM64.tar.gz",
      "ARM": "trivy_{version}_Linux-ARM.tar.gz"
    },
    "Windows": {
      "X64": "trivy_{version}_windows-64bit.zip"
    },
    "macOS": {
      "X64": "trivy_{version}_macOS-64bit.tar.gz",
      "ARM64": "trivy_{version}_macOS-ARM64.tar.gz"
    }
  },
  "extract_patterns": ["trivy", "trivy.exe"]
}
