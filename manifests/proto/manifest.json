{
  "repo": "moonrepo/proto",
  "pattern": "proto_cli-{arch}-{os}",
  "mappings": {
    "arch": { "X64": "x86_64", "ARM64": "aarch64", "ARM": "arm" },
    "os": {
      "Linux": "unknown-linux-gnu",
      "Windows": "pc-windows-msvc",
      "macOS": "apple-darwin"
    }
  },
  "extract_patterns": ["proto", "proto.exe"]
}
